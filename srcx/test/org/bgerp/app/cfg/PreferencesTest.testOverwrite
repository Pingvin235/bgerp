configRow=<<END
   tr = "<tr processid='" + process.getId() + "' ";  
   if (col24.contains("консультация"))
     tr += " bgcolor='green'";
   tr += ">";
   tr += "<td>" + col1 + "</td>";
   tr += "<td>" + col3 + "</td>";
   tr += "<td>" + col4 + "</td>";
   tr += "<td>" + col5 + "</td>";
